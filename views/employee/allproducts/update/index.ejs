<!DOCTYPE html>
<html>
  <head>
    <title>EDIT</title>
  </head>
  <body>
    <form method='post'>
      <table>
        <tr>
          <td> ID </td>
          <td> <input type='text' name='n' value='<%= list[0].id %>' readonly> </td>
        </tr>

        <tr>
          <td> NAME </td>
          <td> <input type='text' name='a' value='<%= list[0].name %>' ></td>
        </tr>

        <tr>
          <td> Quantity </td>
          <td> <input type='number' name='b' value='<%= list[0].quant %>' > </td>
        </tr>

        <tr>
          <td> PRICE: </td>
          <td> <input type='number' name='c' value='<%= list[0].price %>' > </td>
        </tr>
      </table>
    <input type="submit" value='Update'>
    </form>
  </body>
</html>
