<!DOCTYPE html>
<html>
  <head>
    <title>DELETE</title>
  </head>
  <body>
    <form method='post'>
    <table>
      <tr>
        <td> ID </td>
        <td> <input type='text' name='n' value='<%= list[0].id %>' readonly> </td>
      </tr>

      <tr>
        <td> NAME </td>
        <td> <input type='text' name='p' value='<%= list[0].name %>' readonly></td>
      </tr>

      <tr>
        <td> Quantity </td>
        <td> <input type='text' name='d' value='<%= list[0].quant %>' readonly> </td>
      </tr>

      <tr>
        <td> PRICE: </td>
        <td> <input type='text' name='b' value='<%= list[0].price %>' readonly> </td>
      </tr>
    </table>
    <input type="submit" name="y" value='Yes'>
    <input type="submit" name="n" value='No'>
    <a href="/logout"> LOGOUT </a>
    </form>
  </body>
</html>
